# -*- tab-width: 2; x-auto-expand-tabs: true; indent-tabs-mode: nil; coding: utf-8; mode: yaml; -*-

################################################################################

root:
  class: UIView
  size: [ 320, 416 ]
  subviews:
  - class: UIImageView
    id: image
    image: { name: 'Button.png', capInsets: { left: 7, right: 7, top: 7, bottom: 7 } }
  constraints:
  - visual: 'H:|-0-[image]-0-|'
  - visual: 'V:|-0-[image]-0-|'
