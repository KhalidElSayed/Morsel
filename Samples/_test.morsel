# -*- tab-width: 2; x-auto-expand-tabs: true; indent-tabs-mode: nil; coding: utf-8; mode: yaml; -*-

################################################################################

root:
  class: UIView
  id: root
  backgroundColor: red
  subviews:
  - class: UILabel
    id: label1
    text: 'Hello world'
    font: { name: 'HelveticaNeue-Bold', size: 17 }
    backgroundColor: blueColor
  constraints:
  - visual: 'H:[root(320)]'
  - visual: 'V:[root(416)]'
  - visual: 'H:|-10-[label1]-10-|'
  - visual: 'V:|-10-[label1]'

