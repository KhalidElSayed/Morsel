# -*- tab-width: 2; x-auto-expand-tabs: true; indent-tabs-mode: nil; coding: utf-8; mode: yaml; -*-

################################################################################

root:
  class: UIView
  size: [ 320, 416 ]
  backgroundColor: black
  subviews:
  - class: UIImageView
    id: image
    contentMode: UIViewContentModeScaleAspectFill
    image: { url: 'http://lorempixel.com/320/416/' }
  constraints:
  - visual: 'H:|-0-[image]-0-|'
  - visual: 'V:|-0-[image]-0-|'
